<% reset_cycle %>
<table class="twikiTable" cellpadding="0" cellspacing="0" width="685">
  <tr>
    <% unless skip_course_column %>
    <th>Course</th>
    <% end %>
    <th>Task</th>
    <th>Name</th>
    <th>Team/Individual Name</th>
    <th>Date</th>
    <th>Feedback Received?</th>
<!--
    <th>File</th>
-->
  </tr>

  <% presentations.each do |presentation| %>
    <tr class=<%= cycle('twikiTableOdd', 'twikiTableEven')%> >
      <% unless skip_course_column %>
      <td><%= presentation.course.name %></td>
      <% end %>
      <td><%= presentation.task_number %></td>
      <td><%= presentation.name %></td>
      <td><%= presentation.team.name unless presentation.team.nil? %></td>
      <td><%= presentation.present_date  %></td>
      <td><%= link_to "view feedback", presentation_feedback_path(:id => presentation.id)  %> </td>
      <td><%=""%> </td>
<!--      <td>

      </td>
-->
    </tr>
  <% end %>
</table>
