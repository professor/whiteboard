<% content_for :title, "Grade Deliverable" %>

<% if !@deliverable.assignment.can_submit? %>
  <%= render "deliverables/unsubmittable_grading_form" %>
<% elsif @deliverable.is_team_deliverable? %>
  <%= render "deliverables/team_grading_form" %>
<% else %>
  <%= render "deliverables/individual_grading_form" %>
<% end %>